{
  "/login/*": {
    "secure": false,
    "target": "https://github.com",
    "changeOrigin": true,
    "logLevel": "debug"
  },
  "/user/*": {
    "secure": false,
    "target": "https://api.github.com",
    "changeOrigin": true,
    "logLevel": "debug"
  },
  "/api/*": {
    "secure": false,
    "target": "http://localhost:8080",
    "changeOrigin": true,
    "logLevel": "debug"
  }
}
